	section#contact
		.container
			.row
				.col-lg-12.text-center
					h2.section-heading Contact Us
					h3.section-subheading.text-muted Lorem ipsum dolor sit amet consectetur.
			.row
				.col-lg-12
					form(name="sentMessage", id="contactForm",novalidate)
						.row
							.col-md-6
								.form-group
									input.form-control(type="text" placeholder="YOUR NAME *", id="name", requiere data-validation-required-message="Please enter your name.")
									p.help-block.text-danger
								.form-group
									input.form-control(type="email" placeholder="YOUR EMAIL *", id="email", requiere data-validation-required-message="Please enter your email address.")
